#include "Game.h"

#define NULL 0

#include <iostream>

using std::cout ;
using std::endl ;

int main(int argc , char* argv[])
{
	Game* game = NULL ;
	game = new Game() ;
	
	game->init() ;

	game->run() ;

	if(game != NULL)
	{
		delete game ;
		game = NULL ;
	}
	
	cout << "I Ran" << endl ;

	return 0 ;
}
